package entities;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

@Entity
public class Product {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	@Column(name="ProductId")
	private int id;
	
	@Column(name = "MetaDataId")
	private String metadataId;
	
	@Column(name="OriginalPrice")
	private String originalPrice;
	
	@Column(name="FinalPrice")
	private String finalPrice;
	
	@Column(name="DiscountId")
	private String discountId;

	@Column(name = "CategoryId")
	private String categoryId;

	public Product() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Product(String metadataId, String originalPrice, String finalPrice, String discountId, String categoryId) {
		super();
		this.metadataId = metadataId;
		this.originalPrice = originalPrice;
		this.finalPrice = finalPrice;
		this.discountId = discountId;
		this.categoryId = categoryId;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getMetadataId() {
		return metadataId;
	}

	public void setMetadataId(String metadataId) {
		this.metadataId = metadataId;
	}

	public String getOriginalPrice() {
		return originalPrice;
	}

	public void setOriginalPrice(String originalPrice) {
		this.originalPrice = originalPrice;
	}

	public String getFinalPrice() {
		return finalPrice;
	}

	public void setFinalPrice(String finalPrice) {
		this.finalPrice = finalPrice;
	}

	public String getDiscountId() {
		return discountId;
	}

	public void setDiscountId(String discountId) {
		this.discountId = discountId;
	}

	@Override
	public String toString() {
		return "Product [metadataId=" + metadataId + ", originalPrice=" + originalPrice + ", finalPrice=" + finalPrice
				+ ", discountId=" + discountId + ", categoryId=" + categoryId + "]";
	}
	
	
	

}
